package Tuesday;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Map<Integer, Integer> m = new Map<>();
		m.resize(2);
		for(int i = 0; i < 9; i ++) {
			m.put(i, i*i);
		}
		System.out.println(m);
		m.put(10, 100);
		System.out.println(m);
		m.remove(6);
		m.remove(5);
		m.remove(1);
		m.remove(4);
		m.remove(2);
		m.remove(3);
		System.out.println(m);
	}

}
