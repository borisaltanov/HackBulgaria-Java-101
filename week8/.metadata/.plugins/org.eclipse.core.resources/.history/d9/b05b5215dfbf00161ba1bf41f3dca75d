package dungeonsAndZombies;

public abstract class Tool {
	private String name;
	private int damage;
	
	public Tool(String name, int damage) {
		this.name = name;
		this.damage = damage;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getDamage() {
		return damage;
	}

	public void setDamage(String damage) {
		this.damage = damage;
	}
}
