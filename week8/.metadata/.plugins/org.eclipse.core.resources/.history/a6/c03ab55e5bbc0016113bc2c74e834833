package daaa;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {1, 3, 5, 5, 7};
		int b[] = {2, 4, 5, 6, 8};
		 
		Arrays.sort(a);
		Arrays.sort(b);
		 
		int i = 0;
		int j = 0;
		 
		while (i < a.length && j < b.length) {
		    if (a[i] == b[j]) {
		        System.out.println(a[i]);
		        i++;
		        j++;
		    } else if (a[i] > b[j]) {
		        j++;
		    } else {
		        i++;
		    }
		}
	}

}
