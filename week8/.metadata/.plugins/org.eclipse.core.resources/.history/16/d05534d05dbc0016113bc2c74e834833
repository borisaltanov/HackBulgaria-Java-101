package daaa;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {1, 3, 3, 5, 7};
		int b[] = {2, 4, 5, 6, 8};
		 
		Arrays.sort(a);
		Arrays.sort(b);
		 
		int i = 0;
		int j = 0;
		 
		while (j < b.length) {
		    if (a[i] < b[j]) {
		        System.out.print(a[i] + " ");
		        i++;
		    } else if (a[i] > b[j]) {
		        System.out.print(b[j] + " ");
		    	j++;
		    } else {
		    	System.out.print(a[i] + " ");
		       i++;
		       j++;
		    }
		    int temp = a[i];
			if (a[i] == temp) {
				i++;
			}
			if (b[j] == temp) {
				j++;
			}
		}
	}

}
