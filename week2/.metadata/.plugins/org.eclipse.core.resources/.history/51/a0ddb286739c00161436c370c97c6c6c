		String [] functionName = new String [] {};
		String [] argumentName = new String [] {};
		String [] functionBody = new String [] {};
		String composition;
		int argument;
		
		Scanner scanner = new Scanner(System.in);
		int numberOfFunctions = scanner.nextInt();
		scanner.nextLine();
		for (int i = 0; i < numberOfFunctions; i++) {
			String line = scanner.nextLine();
			String [] functionAndBody = line.split(" = ");
			System.out.println(functionAndBody[0]);
			String [] functionAndArgument = functionAndBody[0].split(" ");
			System.out.println(functionAndArgument.toString());
			functionName[i] = functionAndArgument[0];
			argumentName[i] = functionAndArgument[1];
			functionBody[i] = functionAndBody[1];
		}
		composition = scanner.nextLine();
		argument = scanner.nextInt();
		scanner.close();
		
		for (int i = 0; i < 2; i++) {
			System.out.println(functionName[i] + " " + argumentName[i] + " " + functionBody[i]);
		}
